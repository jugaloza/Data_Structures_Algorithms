    "N=int(input())\n",
    "def staircase(N):\n",
    "    if N==1 or N==2:\n",
    "        return N\n",
    "    elif N==3:\n",
    "        return N+1\n",
    "    a=staircase(N-1)\n",
    "    b=staircase(N-2)\n",
    "    c=staircase(N-3)\n",
    "    return a+b+c\n",
    "print(staircase(N))"
   ]
  }
